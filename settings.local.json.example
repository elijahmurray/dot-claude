      "Bash(# Get list of changed files\nCHANGED_FILES=$(git diff --name-only main...HEAD)\n\n# Determine change type\nDEV_PATTERNS=\"\".claude/|scripts/|test_|pytest|CLAUDE.md|DATABASE_SETUP|AUTHENTICATION_SETUP|requirements-dev|package-lock|tsconfig|eslint|prettier|.gitignore|Makefile|docker|.env.example\"\"\nUSER_PATTERNS=\"\"app/api/|app/services/|app/tools/|frontend/app/|frontend/components/|app/agents/\"\"\n\nIS_DEV_CHANGE=false\nfor file in $CHANGED_FILES; do\n    if echo \"\"$file\"\" | grep -qE \"\"$USER_PATTERNS\"\"; then\n        IS_USER_CHANGE=true\n        break\n    fi\ndone\n\nif [ \"\"$IS_USER_CHANGE\"\" = true ]; then\n    echo \"\"ðŸ“ This is a user-facing change (affects core functionality)\"\"\n    echo \"\"Target: FEATURES.md\"\"\nelse\n    echo \"\"ðŸ“ This is a developer-focused change\"\"\n    echo \"\"Target: DEVELOPER_EXPERIENCE.md\"\"\nfi\n\necho -e \"\"\\nChanged files:\"\"\necho \"\"$CHANGED_FILES\"\")",
